{
    "name": "nextspace",
    "version": "0.1.0",
    "private": "false",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist/",
        "copy-files": "copyfiles -u 1 src/**/*.svg src/*.scss src/**/*.scss dist/",
        "compile": "tsc -p tsconfig.json",
        "compile-watch": "tsc -p tsconfig.json -w",
        "build": "yarn copy-files && yarn compile",
        "build-watch": "yarn copy-files && yarn compile-watch",
        "release": "node build-help.js --release-package-json"
    },
    "dependencies": {
        "clsx": "^2.0.0",
        "i18next": "^23.6.0",
        "next": "13.5.6",
        "nprogress": "^0.2.0",
        "react": "^18",
        "react-dom": "^18",
        "sass": "^1.69.4"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/nprogress": "^0.2.2",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "copyfiles": "^2.4.1",
        "fs-extra": "^11.1.1",
        "eslint": "^8",
        "eslint-config-next": "13.5.6",
        "rimraf": "^5.0.5",
        "typescript": "^5",
        "yargs": "^17.7.2"
    }
}
